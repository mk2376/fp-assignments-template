{
  "$schema": "https://raw.githubusercontent.com/jetify-com/devbox/0.13.4/.schema/devbox.schema.json",
  "packages": [
    "rlwrap@latest",
    "smlnj@latest",
    "go-task@3.38.0" // Latest is broken
  ],
  "shell": {
    "init_hook": [
      // sml
      "alias sml='rlwrap sml'",
      "alias smla='rlwrap -r smlnj'",
      // go-task
      "alias task='go-task'"
    ],
    "scripts": {}
  }
}
